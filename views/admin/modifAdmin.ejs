<%- include('../header.ejs') %>  
 <main>
     <% if (isLogged) {%>
     <p>Vous pouver modifiez votre profil en remplissant tous les champs du formulaire</p>
        <form method="post">
          
            <label for="nom">Nom</label>
            <input id="nom" name="nom" type="text" value="<%=praticien.nom %>" />
            <label for="prenom">Prenom</label>
            <input id="prenom" name="prenom" type="text" value="<%= praticien.prenom %>" />
            <label for="email">Login</label>
            <input type="email" name="login" id="email" value="<%= praticien.login %>" />
            <label for="mdp">Votre mot de passe</label>
            <input type="password" name="mdp" id="mdp" />
            <input type="submit" value="Modifier le profil Admin" />
        </form>
         <% } %>
    </main>
    <%- include('../footer.ejs') %>  